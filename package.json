{
  "name": "square-tab",
  "version": "0.0.0",
  "license": "GPL-3.0",
  "scripts": {
    "parcel": "parcel",
    "build": "rm -rf dist && parcel build --public-url '.' entry/*",
    "watch": "parcel watch --public-url '.' --no-hmr entry/*",
    "zip": "zip -FSr square-tab.zip elements images scripts styles views manifest.json",
    "lint": "eslint scripts elements",
    "fix": "eslint --fix scripts elements && prettier --write '{scripts,styles,elements}/**/*.{js,css}'"
  },
  "devDependencies": {
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3"
  },
  "dependencies": {
    "rxjs": "^6.3.3"
  }
}
