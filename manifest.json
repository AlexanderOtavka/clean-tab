{
  "manifest_version": 2,

  "name": "Clean Tab",
  "description": "A clean, minimalistic new tab page.",
  "version": "1.0",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
    "bookmarks",
    "storage",
    "https://source.unsplash.com/",
    "chrome://favicon/"
  ],

  "browser_action": {
    "default_title": "Clean Tab",
    "default_popup": "popup.html"
  }
}
